{
  "name": "ss-octopus",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#1e1e1e",
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Primary",
      "scope": [
        "entity.name",
        "entity.name.function",
        "entity.other.attribute-name.class.css",
        "keyword.other.special-method.dockerfile",
        "meta.diff.header",
        "meta.selector.css",
        "new.expr"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Secondary",
      "scope": [
        "comment.block.documentation",
        "entity.name.type.enum",
        "entity.name.type.interface",
        "meta.brace",
        "punctuation.definition.comment",
        "punctuation",
      ],
      "settings": {
        "foreground": "#608b4e"
      }
    },
    {
      "name": "Tertiary",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "constant.language.null",
        "constant.other.symbol",
        "meta.class.ruby",
        "meta.module.ruby",
        "meta.object-literal.key",
        "storage.modifier.ts",
        "string",
        "string.regexp",
        "support.type.property-name.json",
        "support.type.property-name.css",
        "variable.object.property",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#6d6d6d"
      }
    },
    {
      "name": "Emphasis",
      "scope": [],
      "settings": {}
    },
    {
      "name": "Subtle",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "support.type"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Accent 1",
      "scope": [
        "entity.other.inherited-class",
        "support.function.go",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Accent 2",
      "scope": [
        "keyword.control.def.ruby",
        "keyword.control.export",
        "keyword.control.flow",
        "keyword.control.go",
        "keyword.control.ruby",
        "keyword.control.start-block.ruby",
        "variable.other.constant",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "Accent 3",
      "scope": [

      ],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#d10000"
      }
    }
  ],
  "colors": {
    "activityBarBadge.background": "#569cd6",
    "activityBarBadge.foreground": "#1e1e1e",
    "editor.inactiveSelectionBackground": "#3a3d41",
    "editor.selectionHighlightBackground": "#add6ff26",
    "editorCursor.foreground": "#d10000",
    "editorGutter.addedBackground": "#608b4e",
    "editorGutter.deletedBackground": "#d10000",
    "editorGutter.modifiedBackground": "#c6bf41",
    "editorIndentGuide.activeBackground": "#d10000",
    "editorIndentGuide.background": "#404040",
    "editorOverviewRuler.addedForeground": "#608b4e",
    "editorOverviewRuler.deletedForeground": "#d10000",
    "editorOverviewRuler.modifiedForeground": "#d7ba7d",
    "gitDecoration.conflictingResourceForeground": "#d10000",
    "gitDecoration.deletedResourceForeground": "#d7ba7d",
    "gitDecoration.ignoredResourceForeground": "#808080",
    "gitDecoration.modifiedResourceForeground": "#608b4e",
    "gitDecoration.untrackedResourceForeground": "#d7ba7d",
    "list.dropBackground": "#383b3d",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#222222",
    "statusBar.foreground": "#c6bf41"
  }
}
